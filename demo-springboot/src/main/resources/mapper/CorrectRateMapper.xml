<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.CorrectRateDao">
	<resultMap id="correctMap" type="com.example.demo.pojo.CorrectRatePojo">
		<result column="business_no" property="businessNo"/>
		<result column="create_date" property="createDate"/>
		<result column="type" property="type"/>
		<result column="name" property="name"/>
		<result column="correct_rate" property="correctRateList"/>
	</resultMap>
	<select id="queryCorrectRateList" parameterType="java.lang.String" resultMap="correctMap">
		SELECT
		b.business_no,
		b.create_date,
		b.type,
		b.name,
		a.correct_rate
		FROM
		icp_video AS a
		LEFT JOIN icp_info AS b ON a.business_no = b.business_no
		where 1=1
		<if test="businessNo !=null">
			b.business_no = #businessNo#
		</if>
		GROUP BY
		b.business_no,
		b.create_date,
		b.type,
		b. NAME,
		a.correct_rate
	</select>

</mapper>
